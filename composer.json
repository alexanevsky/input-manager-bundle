{
    "name": "alexanevsky/input-manager-bundle",
    "type": "symfony-bundle",
    "description": "Provides functions which allows to map incoming data (for example, from JSON) into an object, modify it, validate it, and map it to a model or Doctrine entity",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Lashchevsky",
            "email": "alexander@lashchevsky.me"
        }
    ],
    "require": {
        "php": ">=8.1",
        "alexanevsky/getter-setter-accessor-bundle": "^1.0",
        "doctrine/orm": "^2.6",
        "symfony/config": "^5.4|^6.0",
        "symfony/dependency-injection": "^5.4|^6.0",
        "symfony/serializer": "^5.4|^6.0",
        "symfony/string": "^5.4|^6.0",
        "symfony/translation": "^5.4|^6.0",
        "symfony/validator": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Alexanevsky\\InputManagerBundle\\": "src"
        }
    }
}
